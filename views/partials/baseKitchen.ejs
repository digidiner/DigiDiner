<head>
    <meta charset="UTF-8">
    <title>DigiDiner</title>
    <link rel="stylesheet" href="/css/bootstrap.min.css">

</head>

<body>
    <header>
        <div class="px-3 py-4 text-bg-dark border-bottom">
            <div class="container-fluid justify-content-lg-start">
                <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                    <a href="/"
                        class="d-flex align-items-center my-2 my-lg-0 me-lg-auto text-white text-decoration-none">
                        <img src="images/logoCloseup.png" role="img" alt="LogoClose" width="100" height="84">
                    </a>
                    <ul class="nav col-12 col-lg-auto my-2 justify-content-center my-md-0 text-small">
                        <li>
                            <a href="#" class="nav-link text-secondary">
                                <svg class="bi bi-house-door" width="24" height="24">
                                    <use xlink:href="#house-door" />
                                </svg>
                                Home
                            </a>
                        </li>
                        <li>
                            <a href="#" class="nav-link text-white">
                                <svg class="bi bi-clock-history" width="24" height="24">
                                    <use xlink:href="#clock-history" />
                                </svg>
                                Clock
                            </a>
                        </li>
                        <li>
                            <a href="#" class="nav-link text-white">
                                <svg class="bi bi-list-ul" width="24" height="24">
                                    <use xlink:href="#list-ul" />
                                </svg>
                                Orders
                            </a>
                        </li>
                        <li>
                            <a href="#" class="nav-link text-white">
                                <svg class="bi bi-file-earmark-spreadsheet" width="24" height="24">
                                    <use xlink:href="#file-earmark-spreadsheet" />
                                </svg>
                                Inventory
                            </a>
                        </li>
                        <li>
                            <a href="#" class="nav-link text-white">
                                <svg class="bi bi-map" width="24" height="24">
                                    <use xlink:href="#map" />
                                </svg>
                                Floor Map
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </header>

    <div class="container-fluid vh-100 d-flex align-items-center justify-content-center">
        <div class="row">
            <div class="col-12 text-center">
                <h1>Choose an Option</h1>
            </div>
            <div class="col-4">
                <a href="#" class="btn btn-primary btn-lg btn-block">Button 1</a>
            </div>
            <div class="col-4">
                <a href="#" class="btn btn-secondary btn-lg btn-block">Button 2</a>
            </div>
            <div class="col-4">
                <a href="#" class="btn btn-success btn-lg btn-block">Button 3</a>
            </div>
        </div>
    </div>
    <script src="/js/bootstrap.bundle.min.js"></script>
</body>